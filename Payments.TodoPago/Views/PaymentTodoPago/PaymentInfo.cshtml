@{
    Layout = "";
}
@model Nop.Plugin.Payments.TodoPago.Models.PaymentInfoModel

<table width="100%" cellpadding="0" cellspacing="0">
    <tr>
        <td>
            <input type="radio" name="tp_form_hib" id="tp_form_hib_1" value="F" />
        </td>
        <td style="text-align: center;">
            <label for="tp_form_hib_1"><img src="https://todopago.com.ar/sites/todopago.com.ar/files/pluginstarjeta.jpg" height="60" /></label>
        </td>
    </tr>
    <tr id="tr_billetera">
        <td>
            <input type="radio" name="tp_form_hib" id="tp_form_hib_2" value="T" checked="checked" />
        </td>
        <td style="text-align: center;">
            <label for="tp_form_hib_2"><img src="@Model.UrlBilletera" height="60" /></label>
        </td>
    </tr>
</table>
